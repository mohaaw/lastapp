{
  "kind": "collectionType",
  "collectionName": "invoice_items",
  "info": {
    "singularName": "invoice-item",
    "pluralName": "invoice-items",
    "displayName": "Invoice Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "product_name": {
      "type": "string",
      "required": true
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "price": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "subtotal": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "invoice": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::invoice.invoice",
      "inversedBy": "invoice_items"
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    }
  }
}
